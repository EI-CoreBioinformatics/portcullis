{
    "parameters": {
        "nb_reads": {
            "operator": "gte",
            "value": 1000
        },
        "coverage": {
            "operator": "gt",
            "value": 20
        },
        "canonical_ss": {
            "operator": "in",
            "value": ["C"]
        },
        "intron_size.1": {
            "operator": "gte",
            "value": 1000
        },
        "intron_size.2": {
            "operator": "lte",
            "value": 10000
        },
        "entropy": {
            "operator": "gt",
            "value": 2.0
        },
        "refname": {
            "operator": "not_in",
            "value": ["I"]
        }
    },
    "expression": "refname & canonical_ss & intron_size.2 & (nb_reads | (entropy & coverage & intron_size.1))"
}
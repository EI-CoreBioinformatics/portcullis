prefix=@prefix@
exec_prefix=@exec_prefix@
includedir=@includedir@
libdir=@libdir@

Name: phts
Description: C library for high-throughput sequencing data formats
Version: @PACKAGE_VERSION@
Cflags: -I${includedir}
Libs: -L${libdir} -lphts
Libs.private: -L${libdir} -lphts -lm -ldl -lpthread
Requires.private: zlib
